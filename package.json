{
  "name": "bell-syntax",
  "displayName": "bell syntax",
  "description": "syntax highlighting support for bell, the bach evaluation language for lllls",
  "version": "0.0.3",
  "icon": "./images/bell-icon.png",
  "publisher": "tovarhenao",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "bell",
        "icon": {
          "light": "./images/bell-icon.svg",
          "dark": "./images/bell-icon.svg",
          "*": "./images/bell-icon.svg"
        },
        "aliases": [
          "bell"
        ],
        "extensions": [
          ".bell"
        ],
        "configuration": "./language-configuration.json"
      },
      {
				"id": "llll",
				"icon": {
					"light": "./images/bach.svg",
					"dark": "./images/bach.svg",
					"*": "./images/bach.svg"
				},
				"aliases": [
					"llll"
				],
				"extensions": [
					".llll"
				]
			}
    ],
    "grammars": [
      {
        "language": "bell",
        "scopeName": "source.bell",
        "path": "./syntaxes/bell.tmLanguage.json"
      }
    ]
  }
}