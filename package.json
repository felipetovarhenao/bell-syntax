{
  "name": "bell-syntax",
  "displayName": "bell syntax",
  "description": "syntax highlighting support for bell, the bach evaluation language for lllls",
  "version": "0.0.14",
  "icon": "./images/logo.png",
  "publisher": "tovarhenao",
  "repository": {
    "type": "git",
    "url": "https://github.com/felipetovarhenao/bell-syntax/"
  },
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^18",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.26.0",
    "typescript": "^5.4.2"
  },
  "contributes": {
    "languages": [
      {
        "id": "bell",
        "icon": {
          "light": "./images/logo.svg",
          "dark": "./images/logo.svg",
          "*": "./images/logo.svg"
        },
        "aliases": [
          "bell"
        ],
        "extensions": [
          ".bell"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "midi",
        "icon": {
          "light": "./images/midi.svg",
          "dark": "./images/midi.svg",
          "*": "./images/midi.svg"
        },
        "aliases": [
          "mid",
          "midi"
        ],
        "extensions": [
          ".mid",
          ".midi"
        ]
      },
      {
        "id": "max",
        "icon": {
          "light": "./images/max.svg",
          "dark": "./images/max.svg",
          "*": "./images/max.svg"
        },
        "aliases": [
          "maxpat",
          "maxproj"
        ],
        "extensions": [
          ".maxpat",
          ".maxproj"
        ]
      },
      {
        "id": "llll",
        "icon": {
          "light": "./images/llll.svg",
          "dark": "./images/llll.svg",
          "*": "./images/llll.svg"
        },
        "aliases": [
          "llll"
        ],
        "extensions": [
          ".llll"
        ]
      }
    ],
    "grammars": [
      {
        "language": "bell",
        "scopeName": "source.bell",
        "path": "./syntaxes/bell.tmLanguage.json"
      }
    ]
  }
}